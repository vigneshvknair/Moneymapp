<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.gareebisolutions.moneymapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.gareebisolutions.moneymapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Expense"
            android:label="@string/Expenses">


            <intent-filter>
                <action android:name="com.gareebisolutions.Expense" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>
        <activity android:name=".Income"
            android:label="Income">


            <intent-filter>
                <action android:name="com.gareebisolutions.Income" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>

        <activity android:name=".Viewer"
            android:label="Viewer">


            <intent-filter>
                <action android:name="com.gareebisolutions.Viewer" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>




    </application>

</manifest>
